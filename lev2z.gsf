function lev2z( lev )
  'q dims'
  line = sublin( result, 4 )
  type = subwrd( line, 3 )
  if( type = 'varying' )
    z1 = subwrd( line, 11 )
    z2 = subwrd( line, 13 )
  else
    z1 = subwrd( line, 9 )
    z2 = subwrd( line, 9 )
  endif
  'set lev 'lev
  'q dims'
  line = sublin( result, 4 )
  z    = subwrd( line, 9 )
  'set z 'z1' 'z2
return z
